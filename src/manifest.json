{
  "name": "Download Box",
  "version": "0.1.0",

  "manifest_version": 3,
  "minimum_chrome_version": "84",

  "action": {
    "default_popup": "popup.html",
    "default_title": "Downloads"
  },

  "background": {
    "service_worker": "background.js"
  },

  "permissions": [
    "downloads",
    "downloads.open",
    "downloads.shelf"
  ]
}